package com.kodnest.patternconcept;

public class Solution1 {
	public static void main(String[] args) {
		for(int i =1;i<=5;i++) {                 //vertical star =*****
			System.out.print("*");
		}
		
		
		for(int i=0;i<=5;i++) {
			System.out.println("*");            //horizontal star=*....
		}
		
		//square star
		for(int i=1;i<=5;i++) {
			for(int j=1 ;j<=5;j++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}

}
