package com.kodnest.patternconcept;

class Solutuon26Q10 {
	public static void main(String[] args) {
		for(int i=1;i<=19;i++) {
			for(int j=1;j<=19;j++) {
				if(i==1 && j>10 || i+j==11 && i<10 ||i+j==20 && i<10) {
				System.out.print("*");
				}
				else if(i==19 && j>10 || i==j && j>=10 ||i-j==9 && i>10|| i==10 && j<10) {
					System.out.print("*");
				}
				else {
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}

}
