package com.kodnest.patternconcept;

public class Solution11 {
	public static void main(String[] args) {
	    for (int i = 1; i <= 6; i++) 
	    { 
	    	for(int j =1;j<=6;j++) {
	    		System.out.print("*");
	    	}
	    	System.out.println();
	    }
	    /*for(int w=1;w<=4;w++) {
	    	for(int k =1;k<=w;k++) {
	    		System.out.print("?");
	    	}
	        System.out.println(); 
	    }*/
	    
	}

}
